package string3;

public class WordCountOfAString {
public static void main(String[] args) {
	String s="Iranian regime is celebrating war victory over Israel";
	String[] s1 = s.split(" ");
	int count=0;
	for (int i = 0; i < s1.length; i++) {
		count++;
	}
	//printing the word 
	System.out.println("Number of word present in the given string is : "+count);
}
}

